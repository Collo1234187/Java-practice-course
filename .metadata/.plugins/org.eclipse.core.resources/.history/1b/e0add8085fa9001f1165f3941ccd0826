package printingDuplicate;

public class Duplicate {

	public static void main(String[] args) {
		String sentence ="print all the duplicate characters";
		
		 String duplicates="";
		 
		 String charaters="";
     
		 
		for(int i =0 ;i < sentence.length(); i ++) {
		
		System.out.println(sentence.charAt(i));
	}

}
}