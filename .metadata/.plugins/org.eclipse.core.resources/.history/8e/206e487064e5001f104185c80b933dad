package forLoopStar;

import java.util.Scanner;

public class forLoop {

	public static void main(String[] args) {
		System.out.println("Enter your number?");
		Scanner scan=new Scanner(System.in);
		int number=scan.nextInt();
		
		for(int i = 0;i < number;i++) {
		   for(int j = 0; j < 3;j++) {
			   
			   System.out.print("*");
			   
		   }
		   
     System.out.println();
	}

}
}