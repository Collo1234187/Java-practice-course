
public class DuplicateDetermine {

	public static void main(String[] args) {
		String sentence = "i have been one of the best since the beginning";
	
		String character = "";
		String duplicate = "";
for(int i = 0;i < sentence.length();i++) {

	
	String current = character.toString();
	
	if(character.contains(current)) {
	
	duplicate += current;
	}
character += sentence.charAt(i);
}
System.out.println(duplicate);


	}
}